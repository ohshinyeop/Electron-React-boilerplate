{
  "name": "electron-react-with-jar",
  "version": "0.1.0",
  "private": true,
  "main": "src/main/electron.js",
  "license": "MIT",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.23",
    "@types/node-ssh": "^7.0.1",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "electron-is-dev": "^2.0.0",
    "node-ssh": "^13.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "start-renderer": "cross-env BROWSER=none yarn react-start",
    "postinstall": "electron-builder install-app-deps",
    "start-main": "electron ./dist/electron.js",
    "compile-main": "tsc",
    "start-main-after-renderer": "wait-on http://localhost:3000 && yarn start-main",
    "dev": "yarn compile-main && concurrently -n renderer, main 'npm:start-renderer' 'npm:start-main-after-renderer'",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "prebundle": "cp -r ./dist/electron.js ./dist/backendCommand.js ./dist/preload.js build",
    "bundle:osx-arm": "yarn run build && yarn compile-main  && yarn prebundle && electron-builder build -c.extraMetadata.main=build/electron.js --mac",
    "bundle:osx-64": "yarn run build && yarn compile-main  && yarn prebundle && electron-builder build -c.extraMetadata.main=build/electron.js --mac --x64",
    "bundle:win-64": "yarn run build && yarn compile-main  && yarn prebundle && electron-builder build -c.extraMetadata.main=build/electron.js --win --x64"
  },
  "build": {
    "appId": "example.electron-react-with-jar",
    "productName": "example-electron-react-with-jar",
    "extraResources": [
      {
        "from": "models",
        "to": "models",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "cache",
        "to": "cache",
        "filter": [
          "**/*"
        ]
      }
    ],
    "mac": {
      "icon": "build/logo512.png"
    },
    "win": {
      "icon": "build/logo512.png"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "concurrently": "^8.0.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "electron": "^23.2.1",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "node-sass": "^8.0.0",
    "prettier": "^2.8.7",
    "typescript": "^4.9.5",
    "wait-on": "^7.0.1"
  }
}
